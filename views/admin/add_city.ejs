<%- include('../_layouts/adminheader') %>

<h3 class="page-title">Dodaj mesto</h3>
<a href="/admin/cities" class="btn btn-primary">Nazad na sva mesta</a>
<br><br>


<form method="post" action="/admin/cities/add-city">
    <div class="form-group">
        <label for="">Grad</label>
        <input type="text" class="form-control" name="title" value="<%= title %>" placeholder="grad">
    </div>

    <div class="form-group">
        <label for="">Aerodrom</label>
        <input type="text" class="form-control" name="airportname" value="<%= airportname %>" placeholder="aerodrom">
    </div>

    <div class="form-group">
        <label for="">Skraćenica za URL</label>
        <input type="text" class="form-control" name="slug" placeholder="skraćenica" value="<%= slug %>">
    </div>

    <button class="btn btn-default">Potvrdi</button>

    </form>

  
<%- include('../_layouts/adminfooter') %>
